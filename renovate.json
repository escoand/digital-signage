{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:js-app",
    "group:allNonMajor"
  ],
  "regexManagers": [
    {
      "fileMatch": ["^\\.babelrc\\.json$"],
      "matchStrings": ["\"corejs\":\\s*\\{[^}]*}"],
      "depNameTemplate": "core-js",
      "depTypeTemplate": "dependencies",
      "datasourceTemplate": "npm"
    }
  ]
}
